{
    "name": "website",
    "version": "1.0.0",
    "description": "Zack M Fleischman's Personal Website",
    "main": "webpack.config.js",
    "scripts": {
        "start": "npm-run-all webpack:dev-server",
        "dev": "npm-run-all webpack:dev",
        "prod": "npm-run-all webpack:prod",
        "deploy": "npm-run-all -s prod deploy:all",
        "deploy:all": "npm-run-all -p deploy:code deploy:assets deploy:configs",
        "deploy:code": "aws s3 sync www s3://website-zackmfleischman/ --profile zack",
        "deploy:assets": "aws s3 sync assets s3://website-zackmfleischman/assets/ --profile zack",
        "deploy:configs": "aws s3 sync configs s3://website-zackmfleischman/configs/ --profile zack",
        "webpack:dev-server": "webpack-dev-server --mode development",
        "webpack:dev": "webpack --mode development",
        "webpack:prod": "webpack --mode production",
        "lint": "node ./node_modules/tslint/bin/tslint -c tslint.json -p .",
        "analyzer": "webpack --profile --json > .bundle-analyzer-stats.json && webpack-bundle-analyzer .bundle-analyzer-stats.json"
    },
