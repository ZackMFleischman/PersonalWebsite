{
    "name": "webtool",
    "version": "1.0.0",
    "description": "Strype's 3D Web Experience",
    "main": "webpack.config.js",
    "scripts": {
        "start": "npm-run-all -s enforceBackgroundProcessIsRunning webpack:dev-server",
        "start:http": "npm-run-all -s enforceBackgroundProcessIsRunning webpack:dev-server:http",
        "dev": "npm-run-all -s enforceBackgroundProcessIsRunning webpack:dev",
        "prod": "npm-run-all -s enforceBackgroundProcessIsRunning tests webpack:prod",
        "dumpProdMessage": "echo \"\n!!! Open https://0.0.0.0:9001/dist/devClientIndex.html to view Prod !!!\n\"",
        "webpack:dev": "webpack --mode development",
        "webpack:prod": "webpack --mode production",
        "webpack:dev-server": "npm run webpack:dev-server:https",
        "webpack:dev-server:http": "webpack-dev-server --mode development --https false",
        "webpack:dev-server:https": "webpack-dev-server --mode development --https true",
        "background": "npm-run-all -p startBackgroundProcessIndicator generateAndWatchIndexes",
        "startBackgroundProcessIndicator": "node scripts/isBackgroundProcessRunning/backgroundProcessIndicator.js",
        "warnIfBackgroundProcessIsntRunning": "node scripts/isBackgroundProcessRunning/isBackgroundProcessRunning.js",
        "enforceBackgroundProcessIsRunning": "node scripts/isBackgroundProcessRunning/isBackgroundProcessRunning.js --exit-with-error",
        "startLocalServer:https": "http-server -p9001 --cors --ssl --cert=./ssl/cert.pem --key=./ssl/key.pem",
        "test": "npm run tests",
        "tests": "npm-run-all -s warnIfBackgroundProcessIsntRunning tests-with-coverage lint verify-dependency-graph showTestsPassedMessage",
        "tests-with-coverage": "nyc npm run mocha-tests-strict",
        "coverage": "npm-run-all -c -s tests-with-coverage show-coverage",
        "show-coverage": "(node scripts/openFileInBrowser.js coverage/lcov-report/index.html)&",
        "mocha-tests": "mocha --opts mocha.opt",
        "mocha-tests-strict": "cross-env TS_NODE_COMPILER_OPTIONS={\\\"noUnusedLocals\\\":true} mocha --opts mocha.opt",
        "show-mocha-tests": "(node scripts/openFileInBrowser.js mochawesome-report/mochawesome.html)&",
        "mocha-tests-html": "npm-run-all -c -s mocha-tests show-mocha-tests",
        "lint": "node ./node_modules/tslint/bin/tslint -c tslint.json -p .",
        "lint:fix": "node ./node_modules/tslint/bin/tslint -c tslint.json -p . --fix",
        "dependency-graph-src": "./node_modules/dependency-cruiser/bin/dependency-cruise -v -T dot src/ts/WebtoolMain.ts | dot -T svg > dependency_graph_for_src.svg; open dependency_graph_for_src.svg",
        "dependency-graph-tests": "./node_modules/dependency-cruiser/bin/dependency-cruise -v -T dot tests/**/*.ts | dot -T svg > dependency_graph_for_tests.svg; open dependency_graph_for_tests.svg",
        "dependency-graph-subset": "./scripts/show-dependencies.sh",
        "verify-dependency-graph": "node ./node_modules/dependency-cruiser/bin/dependency-cruise -v -T err src/ts/WebtoolMain.ts tests/**/*.ts",
        "dg": "npm run dependency-graph-src",
        "analyzer": "webpack --profile --json > .bundle-analyzer-stats.json && webpack-bundle-analyzer .bundle-analyzer-stats.json",
        "generateConfigIndex": "node scripts/IndexGenerators/ConfigIndexGenerator.js",
        "generateAssetIndex": "node scripts/IndexGenerators/AssetIndexGenerator.js",
        "generateIndexes": "npm-run-all -s generateConfigIndex generateAssetIndex",
        "watchIndexes": "node scripts/IndexGenerators/watchIndexes.js",
        "generateAndWatchIndexes": "npm-run-all -s generateIndexes watchIndexes",
        "showTestsPassedMessage": "node scripts/dumpTestsSuccessMessage.js",
        "syncAssetsToS3:prod": "aws s3 sync assets/ s3://assets.webtool.strypes.co/assets",
        "syncConfigsToS3:prod": "aws s3 sync configs/ s3://configs.webtool.strypes.co/configs",
        "syncIndexesToS3:prod": "aws s3 sync indexes/ s3://configs.webtool.strypes.co/indexes",
        "deployAssets:prod": "npm-run-all -p syncAssetsToS3:prod syncIndexesToS3:prod",
        "deployConfigs:prod": "npm-run-all -p syncConfigsToS3:prod syncIndexesToS3:prod",
        "deployShaders:prod": "aws s3 sync src/Shaders/ s3://code.webtool.strypes.co/src/Shaders",
        "deployBundles:prod": "aws s3 sync dist/ s3://code.webtool.strypes.co/",
        "deployTestWebtoolClient:prod": "aws s3 cp dist/devClientIndex.html s3://test-webtool-client.strypes.co/",
        "deployCode:prod": "npm-run-all -s deployShaders:prod deployBundles:prod deployTestWebtoolClient:prod",
        "deploy:prod": "npm-run-all -s prod deployCode:prod deployConfigs:prod deployAssets:prod"
    },
    "nyc": {
        "extension": [
            ".ts",
            ".tsx"
        ],
        "include": [
            "src/ts/**/*"
        ],
        "exclude": [
            "**/*.d.ts",
            "src/ts/WebtoolMain.ts",
            "src/ts/WebtoolEmbedderMain.ts",
            "src/ts/Bootstrapper/Bootstrapper.ts",
            "src/ts/Materials/BabylonPBRMaterialShim.ts"
        ],
        "reporter": [
            "lcov",
            "text-summary"
        ],
        "all": true
    },
    "keywords": [],
    "author": "Zack M Fleischman",
    "license": "",
    "devDependencies": {
        "@types/chai": "^4.1.4",
        "@types/chai-as-promised": "^7.1.0",
        "@types/chai-things": "0.0.33",
        "@types/easy-table": "0.0.32",
        "@types/glob": "^7.1.1",
        "@types/js-yaml": "^3.11.2",
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.9.4",
        "@types/react": "^16.4.18",
        "@types/react-dom": "^16.0.9",
        "@types/react-redux": "^6.0.11",
        "@types/url-join": "^0.8.2",
        "@types/url-parse": "^1.4.1",
        "chai": "^4.1.2",
        "chai-as-promised": "^7.1.1",
        "chai-things": "^0.2.0",
        "chokidar": "^2.0.4",
        "colors": "^1.3.3",
        "cross-env": "^5.2.0",
        "css-loader": "^1.0.0",
        "dependency-cruiser": "^4.3.2",
        "fork-ts-checker-webpack-plugin": "^0.5.0",
        "glob": "^7.1.3",
        "html-webpack-plugin": "^3.2.0",
        "http-server": "^0.11.1",
        "mocha": "^5.2.0",
        "mochawesome": "^3.0.3",
        "node": "^10.10.0",
        "node-ip": "^0.1.2",
        "node-sass": "^4.9.2",
        "npm-run-all": "^4.1.3",
        "nyc": "^13.0.1",
        "opn": "^5.4.0",
        "ps-node": "^0.1.6",
        "raw-loader": "^0.5.1",
        "sass-loader": "^7.0.3",
        "source-map-support": "^0.5.9",
        "style-loader": "^0.21.0",
        "ts-loader": "^4.4.2",
        "ts-node": "^7.0.0",
        "tsconfig-paths": "^3.4.2",
        "tslint": "^5.11.0",
        "tslint-config-prettier": "^1.14.0",
        "tslint-react": "^3.6.0",
        "typescript": "^3.0.0",
        "typings-for-css-modules-loader": "^1.7.0",
        "url-join": "^4.0.0",
        "webpack": "^4.15.1",
        "webpack-bundle-analyzer": "^2.13.1",
        "webpack-cli": "^3.1.0",
        "webpack-dev-server": "^3.1.10"
    },
    "dependencies": {
        "babylonjs": "^3.3.0",
        "babylonjs-loaders": "^3.2.0",
        "easy-table": "^1.1.1",
        "fast-deep-equal": "^2.0.1",
        "immer": "^1.8.0",
        "inversify": "^4.13.0",
        "js-yaml": "^3.12.0",
        "pepjs": "^0.4.3",
        "performance-now": "^2.1.0",
        "react": "^16.6.3",
        "react-dom": "^16.6.3",
        "react-redux": "^6.0.0",
        "redux": "^4.0.1",
        "redux-devtools-extension": "^2.13.7",
        "reflect-metadata": "^0.1.12",
        "ts-error": "^1.0.3",
        "url-parse": "^1.4.4"
    }
}